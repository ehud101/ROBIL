<?xml version="1.0"?>
<package format="2">
  <name>rcldotnet_tests</name>
  <version>0.0.0</version>
  <description>Package containing tests for rcldotnet.</description>
  <maintainer email="samuel@dynorobotics.se">Samuel Lindgren</maintainer>
